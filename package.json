{
  "name": "crud-sqlserver",
  "version": "1.1.1",
  "description": "A comprehensive CRUD SQL Server management system built with Electron, Quasar Framework, and Vue.js - نظام إدارة قواعد البيانات SQL Server شامل",
  "productName": "CRUD SQL Server",
  "author": {
    "name": "MohamedAboElnasrHassan",
    "email": "mohamed.aboelnasr.hassan@gmail.com",
    "url": "https://github.com/MohamedAboElnasrHassan"
  },
  "license": "MIT",
  "homepage": "https://github.com/MohamedAboElnasrHassan/crud-sqlserver#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/MohamedAboElnasrHassan/crud-sqlserver.git"
  },
  "bugs": {
    "url": "https://github.com/MohamedAboElnasrHassan/crud-sqlserver/issues"
  },
  "keywords": [
    "sql-server",
    "database",
    "crud",
    "electron",
    "quasar",
    "vue",
    "typescript",
    "desktop-app",
    "database-management",
    "windows-classic"
  ],
  "type": "module",
  "private": true,
  "main": "dist/electron/electron-main.js",
  "scripts": {
    "// Code Quality": "=== Code Quality & Linting ===",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "fix-all": "npm run lint:fix && npm run format",
    "// Testing": "=== Testing ===",
    "test": "echo \"No test specified\" && exit 0",
    "test:unit": "echo \"Unit tests not configured yet\" && exit 0",
    "test:e2e": "echo \"E2E tests not configured yet\" && exit 0",
    "// Development": "=== Development ===",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "dev:pwa": "quasar dev -m pwa",
    "dev:ssr": "quasar dev -m ssr",
    "// Building": "=== Building ===",
    "build": "quasar build",
    "build:electron": "quasar build -m electron",
    "build:electron:win": "quasar build -m electron -T win32",
    "build:electron:mac": "quasar build -m electron -T darwin",
    "build:electron:linux": "quasar build -m electron -T linux",
    "build:pwa": "quasar build -m pwa",
    "build:ssr": "quasar build -m ssr",
    "build:spa": "quasar build -m spa",
    "// Publishing": "=== Publishing & Release ===",
    "build:electron:publish": "quasar build -m electron -P always",
    "build:electron:publish:draft": "quasar build -m electron -P onTagOrDraft",
    "build:electron:publish:never": "quasar build -m electron -P never",
    "release": "release-it",
    "release:draft": "npm run build:electron:publish:draft",
    "release:win": "quasar build -m electron -T win32 -P always",
    "release:mac": "quasar build -m electron -T darwin -P always",
    "release:linux": "quasar build -m electron -T linux -P always",
    "// Smart Automation": "=== Smart Automation ===",
    "smart-setup": "node scripts/auto-setup.js",
    "smart-commit": "node scripts/auto-commit.js",
    "quick-commit": "npm run fix-all && npm run smart-commit",
    "pre-release": "npm run fix-all && npm run type-check && npm run build",
    "// Utilities": "=== Utilities ===",
    "clean": "quasar clean",
    "clean:all": "node scripts/cleanup.js && npm install",
    "clean:deep": "node scripts/cleanup.js",
    "check-updates": "npm outdated && npm audit",
    "inspect": "quasar inspect",
    "inspect:electron": "quasar inspect -m electron",
    "serve": "quasar serve dist/spa --port 8080 --gzip",
    "serve:electron": "quasar serve dist/electron --port 8080",
    "// Release": "=== Release Management ===",
    "release:minor": "release-it minor",
    "release:major": "release-it major",
    "release:patch": "release-it patch",
    "release:dry": "release-it --dry-run",
    "release:ci": "release-it --ci",
    "// Quasar": "=== Quasar Framework ===",
    "postinstall": "quasar prepare",
    "prepare": "quasar prepare",
    "info": "quasar info",
    "upgrade": "quasar upgrade",
    "upgrade:all": "quasar upgrade --all"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.0.6",
    "decimal.js": "^10.4.3",
    "electron-log": "^5.0.1",
    "electron-updater": "^6.1.7",
    "html2canvas": "^1.4.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "mssql": "^11.0.1",
    "node-forge": "^1.3.1",
    "pinia": "^3.0.1",
    "qrcode": "^1.5.3",
    "quasar": "^2.16.0",
    "vue": "^3.4.18",
    "vue-i18n": "^11.0.0",
    "vue-router": "^4.0.12",
    "winston": "^3.11.0",
    "xml-crypto": "^6.1.2",
    "xml2js": "^0.6.2",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.2",
    "@eslint/js": "^9.14.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@quasar/app-vite": "^2.1.0",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.2",
    "electron": "^36.3.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "prettier": "^3.3.3",
    "release-it": "^19.0.3",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.0.29"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
